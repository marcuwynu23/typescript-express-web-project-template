{
  "compilerOptions": {
    "target": "es2016", 
    "module": "commonjs",  
    "outDir": "./js-generated",                       
    "rootDir": "./",                              
    "moduleResolution": "node",         
    "baseUrl": "./",                                 
    "rootDirs": [],                 
    "removeComments": true,          
    "esModuleInterop": true,          
    "forceConsistentCasingInFileNames": true,      
    "strict": true,                                 
    "skipLibCheck": true,        
  },
  "include": ["./src/**/*"],   
}
